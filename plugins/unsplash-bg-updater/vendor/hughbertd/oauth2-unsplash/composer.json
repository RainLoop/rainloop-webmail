{
    "name" : "hughbertd/oauth2-unsplash",
    "description": "Unsplash OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on",
        "unsplash"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Hugh Downer",
            "email": "hugh.downer@gmail.com"
        }
    ],
    "require": {
        "php" : ">=5.6.0",
        "league/oauth2-client": ">=1.0"
    },
    "autoload": {
        "psr-4": {
            "Unsplash\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unsplash\\OAuth2\\Client\\Test\\": "test/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    }
}

{
  "name": "unsplash/unsplash",
  "description": "Wrapper to access the Unsplash API and photo library",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Aaron Klaassen",
      "email": "aaron@unsplash.com"
    },

    "hughbertd/oauth2-unsplash": ">=1.0.3"

  },
  "require-dev": {
    "phpunit/phpunit": "~5.1",
    "php-vcr/php-vcr": "dev-master",
    "php-vcr/phpunit-testlistener-vcr": "*",
    "vlucas/phpdotenv": "dev-master",
    "mockery/mockery": "~0.9.0",
    "satooshi/php-coveralls": "dev-master"

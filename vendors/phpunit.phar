     */
    public function __construct()
    {
        if (!defined('HHVM_VERSION')) {
            throw new PHP_CodeCoverage_Exception('This driver requires HHVM');
        }
    }

    /**
     * Start collection of code coverage information.
     */
    public function start()
    {
